{
  "listeners": {
    "*:8000": {
      "pass": "routes"
    }
  },
  "routes": [
   {
      "match": {
        "uri": ["*/static/*", "*/media/*"]
      },
      "action": {
        "share": "/code/"
      }
    },
    {
      "action": {
        "pass": "applications/taskbackend"
      }
    }
  ],
  "applications": {
    "taskbackend": {
      "type": "python",
      "path": "/code",
      "module": "core.wsgi",
      "environment": {
        "ON_PRODUCTION": "True"
      }
    }
  }
}